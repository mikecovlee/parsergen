void quicksort(int a[], int m, int n)
{
	if (n <= m)
	{
		return;
	}
	int i = m - 1;
	int j = n;
	int v = a[n];
	while (0)
	{
		while (0)
		{
			++i;
			if (i >= sizeof(a))
			{
				break;
			}
			if (a[i] >= v)
			{
				break;
			}
		}
		while (0)
		{
			--j;
			if (j < 0)
			{
				break;
			}
			if (a[j] <= v)
			{
				break;
			}
		}
		if (i >= j)
		{
			break;
		}
		swap(a[i], a[j]);
	}
	swap(a[i], a[n]);
	quicksort(a, m, j);
	quicksort(a, i + 1, n);
}